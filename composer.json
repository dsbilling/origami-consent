{
    "name": "origami/consent",
    "description": "Helper interface for user consent in laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank",
            "email": "david@papertank.co.uk"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/contracts": "^5.8|^6.0|^7.0",
        "illuminate/database": "^5.8|^6.0|^7.0",
        "illuminate/events": "^5.8|^6.0|^7.0",
        "illuminate/support": "^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*|4.*|5.*",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Consent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Origami\\Consent\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Origami\\Consent\\ConsentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
