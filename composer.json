{
    "name": "origami/consent",
    "description": "Helper interface for user consent in laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank",
            "email": "david@papertank.co.uk"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/auth": "~5.4|^6.0",
        "illuminate/contracts": "~5.4|^6.0",
        "illuminate/database": "~5.4|^6.0",
        "illuminate/events": "~5.4|^6.0",
        "illuminate/queue": "~5.4|^6.0",
        "illuminate/support": "~5.4|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Consent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Origami\\Consent\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Origami\\Consent\\ConsentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
